{
  "name": "next-router-guards",
  "version": "2.0.0",
  "description": "next-router-guards",
  "main": "dist/cjs/lib/index.js",
  "module": "dist/esm/lib/index.js",
  "bin": {
    "nrg": "./dist/cjs/scripts/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "rm -rf dist/ && npm run prettier:fix && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "clean": "rm -rf dist",
    "prettier:fix": "prettier --write src/",
    "test": "npm run build && npm pack --pack-destination ~"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makskiyko/next-router-guards.git"
  },
  "keywords": [
    "next js",
    "router",
    "guards"
  ],
  "author": "makskiyko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makskiyko/next-router-guards/issues"
  },
  "homepage": "https://github.com/makskiyko/next-router-guards#readme",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.11.19",
    "prettier": "^2.7.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "case": "^1.6.3",
    "chalk": "^2.3.2",
    "cosmiconfig": "^8.0.0",
    "inquirer": "^5.1.0",
    "next": "^13.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0"
  }
}
